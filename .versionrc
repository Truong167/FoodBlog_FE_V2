{
  "types": [
    {
      "type": "feat",
      "section": "✨ Features"
    },
    {
      "type": "fix",
      "section": "🐛 Fixes"
    },
    {
      "type": "chore",
      "section": "🧹 Chores"
    },
    {
      "type": "docs",
      "section": "📝 Docs"
    },
    {
      "type": "style",
      "section": "🎨 Styles"
    },
    {
      "type": "refactor",
      "section": "🛠 Refactors"
    },
    {
      "type": "perf",
      "section": "⚡ Performance"
    },
    {
      "type": "test",
      "section": "✅ Tests"
    }
